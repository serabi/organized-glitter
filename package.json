{
  "name": "organized-glitter",
  "private": true,
  "version": "0.0.0",
  "description": "A React application for diamond painting project management with progress tracking, inventory management, and project organization features",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "keywords": [
    "diamond-painting",
    "craft-management",
    "project-tracker",
    "react",
    "typescript",
    "pocketbase"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/organized-glitter.git"
  },
  "homepage": "https://organizedglitter.app",
  "bugs": {
    "url": "https://github.com/your-username/organized-glitter/issues"
  },
  "author": {
    "name": "Organized Glitter Team",
    "url": "https://organizedglitter.app"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "rollup": "npm:@rollup/wasm-node",
    "esbuild": ">=0.21.0",
    "path-to-regexp": ">=6.2.0",
    "undici": "^5.28.5",
    "glob": "^10.0.0",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4.15"
  },
  "vitest": {
    "environment": "jsdom",
    "setupFiles": [
      "./test/setup.ts"
    ],
    "globals": true,
    "coverage": {
      "reporter": [
        "text",
        "json",
        "html"
      ],
      "exclude": [
        "node_modules/",
        "**/*.d.ts",
        "**/__tests__/**",
        "**/test-utils/**"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:vercel": "vercel dev",
    "build": "tsc -b && vite build",
    "build:with-sitemap": "npm run generate:sitemap:js && vite build",
    "build:analyze": "npm run build && npx tsx scripts/analyze-bundle.ts",
    "bundle:monitor": "npx tsx scripts/bundle-size-monitor.ts",
    "build:monitor": "npm run build && npm run bundle:monitor",
    "perf:benchmark": "node scripts/performance-benchmark.js",
    "perf:bundle": "npm run build && npx webpack-bundle-analyzer dist/assets/index-*.js --mode static --report dist/bundle-report.html --open",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:performance:stats": "npx tsx scripts/performance-test-stats.ts",
    "postinstall": "echo 'Skipping optional dependencies' || true",
    "setup": "rm -rf node_modules package-lock.json && npm install --no-optional",
    "generate:types": "tsx scripts/generate-types.ts",
    "generate:sitemap": "tsx scripts/generate-sitemap.ts",
    "generate:sitemap:js": "node scripts/generate-sitemap.js",
    "db:check": "bash scripts/quick-db-check.sh",
    "test:prod-build": "bash scripts/test-production-build.sh",
    "pb:types": "dotenv -e .env -- sh -c 'pocketbase-typegen --url \"$POCKETBASE_URL\" --email \"$POCKETBASE_ADMIN_EMAIL\" --password \"$POCKETBASE_ADMIN_PASSWORD\" --out src/types/pocketbase.types.ts'",
    "pb:types:local": "dotenv -e .env -- sh -c 'pocketbase-typegen --url \"$LOCAL_POCKETBASE_URL\" --email \"$LOCAL_POCKETBASE_ADMIN_EMAIL\" --password \"$LOCAL_POCKETBASE_ADMIN_PASSWORD\" --out src/types/pocketbase.types.local.ts'",
    "pb:local": "./start-local-dev.sh",
    "pb:create-admin": "bash -c 'if [ -z \"$LOCAL_POCKETBASE_ADMIN_EMAIL\" ] || [ -z \"$LOCAL_POCKETBASE_ADMIN_PASSWORD\" ]; then echo \"Error: LOCAL_POCKETBASE_ADMIN_EMAIL and LOCAL_POCKETBASE_ADMIN_PASSWORD must be set\"; exit 1; fi; ./pocketbase superuser upsert \"$LOCAL_POCKETBASE_ADMIN_EMAIL\" \"$LOCAL_POCKETBASE_ADMIN_PASSWORD\"'",
    "pb:setup-collections": "node scripts/setup-local-collections.js",
    "dev:local": "VITE_POCKETBASE_URL=http://localhost:8090 npm run dev:vercel",
    "dev:prod": "npm run dev:vercel",
    "prebuild": "echo 'Using committed PocketBase types for production build'",
    "predev": "npm run pb:types || echo 'Type generation failed, continuing...'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-visually-hidden": "^1.2.2",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@tanstack/react-virtual": "^3.13.8",
    "@types/papaparse": "^5.3.16",
    "@types/uuid": "^9.0.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/node": "^5.1.16",
    "@vitejs/plugin-react": "^4.3.3",
    "boring-avatars": "^1.11.2",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "papaparse": "^5.5.2",
    "pocketbase": "^0.26.1",
    "posthog-js": "^1.240.6",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^5.0.8",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.15.3",
    "resend": "^4.5.2",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^0.9.3",
    "zod": "^3.23.8",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@eslint/js": "^9.9.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.20",
    "chrome-launcher": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "express": "^4.18.2",
    "globals": "^15.9.0",
    "http-proxy-middleware": "^2.0.6",
    "jsdom": "^25.0.1",
    "pocketbase-typegen": "^1.3.1",
    "postcss": "^8.4.47",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^3.4.14",
    "terser": "^5.39.2",
    "tsx": "^4.19.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "webpack-bundle-analyzer": "^4.10.2",
    "wrangler": "^4.14.4"
  }
}
